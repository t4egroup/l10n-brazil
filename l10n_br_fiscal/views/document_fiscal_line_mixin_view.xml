                                    </group>
                                    <group>
                                        <field force_save="1" invisible="tax_framework == '3'" name="icmssn_percent" readonly="icmssn_tax_id != False"/>
                                        <field force_save="1" invisible="tax_framework == '3'" name="icmssn_credit_value" readonly="icmssn_tax_id != False"/>
                                        <field force_save="1" invisible="tax_framework == '3' or icms_cst_code != '900'" name="icms_reduction" readonly="icms_tax_id != False"/>
