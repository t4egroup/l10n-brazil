                                        <field force_save="1" name="icms_destination_value" readonly="icms_tax_id != False"/>
                                    </group>
                                    <group invisible="icms_cst_code not in ('60', '500')" name="icms_effective" string="ICMS Effective Calculation">
                                        <field attr="{'invisible': [('icms_cst_code', '!=', '500')]}" force_save="1" name="icms_effective_reduction"/>
                                        <field attr="{'invisible': [('icms_cst_code', '!=', '500')]}" force_save="1" name="icms_effective_base"/>
                                        <field attr="{'invisible': [('icms_cst_code', '!=', '500')]}" force_save="1" name="icms_effective_percent"/>
                                        <field attr="{'invisible': [('icms_cst_code', '!=', '500')]}" force_save="1" name="icms_effective_value"/>
                                    </group>
                                </group>
                            </page>
                            <page invisible="tax_icms_or_issqn == 'issqn'" name="ipi" string="IPI">
