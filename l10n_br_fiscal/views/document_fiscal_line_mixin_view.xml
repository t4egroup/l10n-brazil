                                        <field force_save="1" invisible="icms_cst_code not in ('60', '500')" name="icmsfcp_value_wh"/>
                                    </group>
                                    <group invisible="icms_cst_code not in ('10', '30', '70', '90', '201', '202', '203', '500', '900')" name="icms_fcp_st" string="ICMS FCP ST">
                                        <field invisible="icms_cst_code == '500'" name="icmsfcpst_tax_id" options="{'no_create': True, 'no_create_edit': True}"/>
                                        <field force_save="1" invisible="icms_cst_code == '500'" name="icmsfcpst_base" readonly="icmsfcp_tax_id != False"/>
                                        <field force_save="1" invisible="icms_cst_code == '500'" name="icmsfcpst_percent" readonly="icmsfcp_tax_id != False"/>
                                        <field force_save="1" invisible="icms_cst_code == '500'" name="icmsfcpst_value" readonly="icmsfcp_tax_id != False"/>
