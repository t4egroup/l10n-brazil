                                    <field name="issqn_wh_tax_id" options="{'no_create': True, 'no_create_edit': True}"/>
                                </group>
                                <group>
                                    <group>
                                        <field force_save="1" name="issqn_wh_base" readonly="issqn_wh_tax_id != False"/>
                                        <field force_save="1" name="issqn_wh_percent" readonly="issqn_wh_tax_id != False"/>
                                    </group>
                                    <group>
                                        <field force_save="1" name="issqn_wh_reduction" readonly="issqn_wh_tax_id != False"/>
                                        <field force_save="1" name="issqn_wh_value" readonly="issqn_wh_tax_id != False"/>
