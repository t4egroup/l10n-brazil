                                        <field force_save="1" invisible="icms_cst_code in ('60', '500')" name="icmsst_reduction" readonly="icms_tax_id != False"/>
                                        <field force_save="1" invisible="icms_cst_code in ('60', '500')" name="icmsst_base" readonly="icms_tax_id != False"/>
                                        <field force_save="1" invisible="icms_cst_code in ('60', '500')" name="icmsst_value" readonly="icms_tax_id != False"/>
                                        <field force_save="1" invisible="icms_cst_code not in ('60', '500')" name="icmsst_wh_base" readonly="icms_tax_id != False"/>
                                        <field force_save="1" invisible="icms_cst_code not in ('60', '500')" name="icmsst_wh_percent" readonly="icms_tax_id != False"/>
                                        <field force_save="1" invisible="icms_cst_code not in ('60', '500')" name="icmsst_wh_value" readonly="icms_tax_id != False"/>
