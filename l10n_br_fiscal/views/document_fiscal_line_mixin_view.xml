        <field name="model">l10n_br_fiscal.document.line</field>
        <field name="arch" type="xml">
            <form>
                <group invisible="1" name="fiscal_fields">
                    <field force_save="1" invisible="1" name="currency_id"/>
                    <field force_save="1" invisible="1" name="fiscal_operation_type"/>
                    <field force_save="1" invisible="1" name="company_id"/>
                    <field force_save="1" invisible="1" name="partner_id"/>
