                                        <field force_save="1" invisible="tax_framework in ('1', '2') or icms_cst_code not in ('20', '30', '40', '41', '50', '70', '90')" name="icms_relief_id" readonly="icms_tax_id != False and icms_cst_code not in ('20', '30', '40', '41', '50', '70', '90')"/>
                                        <field force_save="1" invisible="tax_framework in ('1', '2') or icms_cst_code not in ('20', '30', '40', '41', '50', '70', '90')" name="icms_relief_value" readonly="icms_tax_id != False and icms_cst_code not in ('20', '30', '40', '41', '50', '70', '90')"/>
